/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

image {
    width:100%;
    max-width:100%;
    max-height: 100%;
}
.text-overflow{
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow:hidden
}
/**列表**/
.list-wrap {
    padding:20rpx;
}
.list-wrap .list .list-item{
    display:flex;
    margin-bottom:20rpx;
}
.list-wrap .list-item .img {
    width:300rpx;
    height:230rpx;
    overflow:hidden;
    margin-right:20rpx;
}
/**固定头部**/
.fixed-title {
    height: 80rpx;
    width:100%;
}
.fixed-title .content {
    position: fixed;
    z-index: 999;
    left: 0;
    top: 0;
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 36rpx;
    text-align: center;
    background-color: #87cefa;
    color: #fff;
    font-weight: bold;
}
